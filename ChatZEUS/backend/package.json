{
  "name": "chatzeus-backend",
  "version": "1.0.0",
  "description": "ChatZEUS Backend - نظام الذكاء الاصطناعي متعدد الوكلاء",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "build": "npm run build:clean && npm run build:copy",
    "build:clean": "rimraf dist",
    "build:copy": "copyfiles -u 1 \"src/**/*\" dist",
    "docker:build": "docker build -t chatzeus-backend .",
    "docker:run": "docker run -p 3000:3000 chatzeus-backend"
  },
  "keywords": [
    "ai",
    "multi-agent",
    "chatbot",
    "openai",
    "anthropic",
    "google",
    "mongodb",
    "nodejs",
    "express"
  ],
  "author": "ChatZEUS Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "gridfs-stream": "^1.1.1",
    "socket.io": "^4.7.4",
    "axios": "^1.6.2",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "@google/generative-ai": "^0.2.1",
    "rate-limiter-flexible": "^3.0.8",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer-gridfs-storage": "^5.0.2",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "rimraf": "^5.0.5",
    "copyfiles": "^2.4.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
